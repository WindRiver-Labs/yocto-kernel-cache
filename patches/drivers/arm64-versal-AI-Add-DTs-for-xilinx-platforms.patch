+	xlnx,usb-reset-mode = <0x0>;
+};
+
+&dwc3_0 { /* USB 2.0 host */
+	status = "okay";
+	dr_mode = "host";
+	maximum-speed = "high-speed";
+	snps,dis_u2_susphy_quirk;
+	snps,dis_u3_susphy_quirk;
+	snps,usb3_lpm_capable;
+};
+
+&qspi {
+        status = "okay";
+        num-cs = <1>;
+        spi-tx-bus-width = <1>;
+        spi-rx-bus-width = <4>;
+        #address-cells = <1>;
+        #size-cells = <0>;
+        is-dual = <1>;
+        flash@0 {
