 			};
 
-			uio_xor0 {
+			uio_xor0: uio-xor0 {
 				compatible = "marvell,uio-xor-v2";
 				xor_access = <&xor0>;
 			};
 
-			uio_xor1 {
+			uio_xor1: uio-xor1 {
 				compatible = "marvell,uio-xor-v2";
 				xor_access = <&xor1>;
 			};
 
-			uio_xor2 {
+			uio_xor2: uio-xor2 {
 				compatible = "marvell,uio-xor-v2";
