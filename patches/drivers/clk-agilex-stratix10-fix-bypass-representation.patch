 	{ AGILEX_BOOT_CLK, "boot_clk", boot_mux, ARRAY_SIZE(boot_mux), 0,
@@ -105,7 +140,7 @@ static const struct stratix10_perip_cnt_clock agilex_main_perip_cnt_clks[] = {
