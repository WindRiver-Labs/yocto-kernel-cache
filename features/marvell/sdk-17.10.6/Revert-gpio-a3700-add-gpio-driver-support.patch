-		pin = GPIO_A3700_REG_PIN_OFF(pin);
-
-		msk = 1 << pin;
-		if (io_conf & msk)
-			is_out = true;
-		else
-			is_out = false;
-
-		seq_printf(s, " gpio-%-3d (%-20.20s)", chip->base + pin, label);
-
-		if (is_out) {
-			seq_printf(s, " out %s\n",
-				   out & msk ? "hi" : "lo");
-			continue;
-		}
-
-	}
-}
-
 #else
 #define mvebu_gpio_dbg_show NULL
 #endif
