 static void mvebu_gpio_irq_ack(struct irq_data *d)
@@ -992,45 +834,6 @@ static void mvebu_gpio_dbg_show(struct seq_file *s, struct gpio_chip *chip)
 		seq_printf(s, " (%s)\n", cause & msk ? "pending" : "clear  ");
