+static struct amba_driver axim_driver = {
+	.drv = {
+		.name   = "coresight-axim",
+	},
+	.probe		= axim_probe,
+	.remove		= axim_remove,
+	.id_table	= axim_ids,
+};
+
+module_amba_driver(axim_driver);
