+	count  = readl_relaxed(axim->base + AXI_MON_CH_COUNT(axim->curr_chan));
+	reload = readl_relaxed(axim->base + AXI_MON_CH_RLD(axim->curr_chan));
+	return scnprintf(buf, PAGE_SIZE, "%#lx\n", count - reload);
+}
