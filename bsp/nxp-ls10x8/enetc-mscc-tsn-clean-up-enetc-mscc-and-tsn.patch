-		bytes	+= priv->rx_ring[i]->stats.bytes;
-	}
-
-	stats->rx_packets = packets;
-	stats->rx_bytes = bytes;
-	bytes = 0;
-	packets = 0;
-
-	for (i = 0; i < priv->num_tx_rings; i++) {
-		packets += priv->tx_ring[i]->stats.packets;
-		bytes	+= priv->tx_ring[i]->stats.bytes;
-	}
