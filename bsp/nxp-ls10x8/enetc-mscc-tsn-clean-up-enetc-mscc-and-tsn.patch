-	struct ocelot_port *ocelot_port;
-
-	for (port = 0; port < FELIX_MAX_NUM_PHY_PORTS; port++) {
-		ocelot_port = ocelot->ports[port];
-		ocelot_port_rmwl(
-			ocelot_port,
-		    DEV_GMII_MM_STATISTICS_MM_STATUS_PRMPT_ACTIVE_STICKY,
-		    DEV_GMII_MM_STATISTICS_MM_STATUS_PRMPT_ACTIVE_STICKY,
-		    DEV_GMII_MM_STATISTICS_MM_STATUS);
-	}
-}
