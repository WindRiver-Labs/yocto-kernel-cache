-
-		err = enetc_set_cls_entry(priv->si, &rxnfc->fs, true);
-		if (err)
-			return err;
-		priv->cls_rules[rxnfc->fs.location].fs = rxnfc->fs;
-		priv->cls_rules[rxnfc->fs.location].used = 1;
-		break;
-	case ETHTOOL_SRXCLSRLDEL:
-		if (rxnfc->fs.location >= priv->si->num_fs_entries)
