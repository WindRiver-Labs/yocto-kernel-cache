-struct net_device_stats *enetc_get_stats(struct net_device *ndev)
-{
-	struct enetc_ndev_priv *priv = netdev_priv(ndev);
-	struct net_device_stats *stats = &ndev->stats;
-	unsigned long packets = 0, bytes = 0;
